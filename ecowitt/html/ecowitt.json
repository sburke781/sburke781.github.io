{
  "name": "Ecowitt HTML template repository",
  "templates": {
    "2":  "<i class=\"ewi-air\" style=\"color:#${aqiColor}\"></i> ${aqiDanger}<br>AQI: ${aqi}<br>PM2.5: ${pm25} &micro;g/m&sup3;",
    "2A": "<i class=\"ewi-air\" style=\"color:#${aqiColor_avg_24h}\"></i> ${aqiDanger_avg_24h}<br>AQI: ${aqi_avg_24h}<br>PM2.5: ${pm25_avg_24h} &micro;g/m&sup3;",      
